#!/usr/bin/env bash
set -e

git clone https://github.com/SpikeInterface/spikeinterface.git
cd spikeinterface
git checkout 969e5a8be4db31f33c4caa98ad5ef6a3c81a4c07
pip install --no-cache-dir .[full,widgets]
cd ..


git clone https://github.com/catalystneuro/neuroconv.git
cd neuroconv
git checkout 46c8a5d640f933a86e57a9cb4c2e95d84c135659
pip install --no-cache-dir .
cd ..
